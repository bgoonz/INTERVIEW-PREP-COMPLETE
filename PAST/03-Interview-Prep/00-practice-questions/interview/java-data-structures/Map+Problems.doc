String fractionToDecimal(int n, int d)
{
    //  Numerator =3D 1, Denominator =3D 2 -&gt; "0.5"
    //  Numerator =3D 2, Denominator =3D 1 -&gt; "2"
    //  Numerator =3D 2, Denominator =3D 3 -&gt; "0.(6)"
/*
Consider: 2/3
                    map
rem     result  key     value
20      0.      20      2
*/
    if(n =3D=3D 0)
    {
        return "0";
    }
    if(d =3D=3D 0)
    {
        return "";
    }
    String result =3D "";
    if((n &lt; 0) ^ (d &lt; 0))
    {
        result +=3D "-";
    }
    n =3D Math.abs(n);
    d =3D Math.abs(d);
    result +=3D n / d;
    //  Remainder
    int rem =3D n % d * 10;
    if(rem =3D=3D 0)
    {
        return result;
    }
    result +=3D ".";
    Map&lt;Integer, Integer&gt; map =3D new HashMap&lt;&gt;();
    while(rem !=3D 0)
    {
        if(map.containsKey(rem))
        {
            int beg =3D map.get(rem);
            String part1 =3D result.substring(0, beg);
            String part2 =3D result.substring(beg);
            result =3D part1 + "(" + part2 + ")";
            return result;
        }
        map.put(rem, result.length());
        result +=3D rem / d;
        rem =3D rem % d * 10;
    }
    return result;
}</pre>=20
