eMap&lt;&gt;();
    for(String s : messages)
    {
        String[] a =3D s.split("\\s+");
        String id =3D a[0];
        String message =3D s.substring(id.length()).trim();
        if(message.matches("^[\\d ]*$"))
        {
            numberMessageMap.computeIfAbsent(message, x -&gt; new TreeSet&l=
t;&gt;()).add(id);
        }
