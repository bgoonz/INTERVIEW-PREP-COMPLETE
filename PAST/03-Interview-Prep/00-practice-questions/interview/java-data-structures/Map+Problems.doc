        map.merge(c, 1, Integer::sum);
        if(map.size() &gt; n)
        {
            max =3D Math.max(max, i - start);
            while(map.size() &gt; n)
            {
                c =3D s.charAt(start);
                int count =3D map.get(c);
                if(count &gt; 1)
                {
                    map.put(c, count - 1);
                }
                else
                {
                    map.remove(c);
                }
                start++;
            }
        }
    }
    return Math.max(max, s.length() - start);
}</pre>=20
