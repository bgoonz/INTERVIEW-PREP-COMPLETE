void bubbleSort(int[] a)
{
    boolean swapped;
    do
    {
        swapped =3D false;
        for(int i =3D 1; i &lt; a.length; ++i)
        {
            if(a[i - 1] &gt; a[i])
            {
                swapped =3D true;
                swap(a, i, i - 1);
            }
        }
    } while(swapped);
