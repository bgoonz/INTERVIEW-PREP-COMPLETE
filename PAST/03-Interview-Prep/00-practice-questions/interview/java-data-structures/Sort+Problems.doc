void mergeSort(int[] a)
{
    if(a.length &lt; 2)
    {
        return;
    }
    int mid =3D a.length / 2;
    int[] left =3D new int[mid];
    int[] right =3D new int[a.length - mid];
    System.arraycopy(a, 0, left, 0, left.length);
    System.arraycopy(a, mid, right, 0, right.length);
    mergeSort(left);
    mergeSort(right);
    merge(a, left, right);
}


private static void merge(int[] dst, int[] left, int[] right)
{
    int dstPos =3D 0, leftPos =3D 0, rightPos =3D 0;
    while(leftPos &lt; left.length &amp;&amp; rightPos &lt; right.length)
    {
        if(left[leftPos] &lt; right[rightPos])
        {
            dst[dstPos++] =3D left[leftPos++];
        }
        else
        {
            dst[dstPos++] =3D right[rightPos++];
        }
    }
    while(leftPos &lt; left.length)
    {
        dst[dstPos++] =3D left[leftPos++];
    }
    while(rightPos &lt; right.length)
    {
        dst[dstPos++] =3D right[rightPos++];
    }
